{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["api", "./", "services","config"],
        "exclude": ["node_modules","out","personal.test.js"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "outputSourceFiles": true,
        "systemName"      : "Node-LoL",
        "footer"          : "node.io",
        "copyright"       : "Node-LoL Copyright Â© 2014 The contributors to the Node-LoL project.",
        "navType"         : "vertical",
        "theme"           : "cerulean",
        "linenums"        : true,
        "collapseSymbols" : false,
        "inverseNav"      : true,
        "syntaxTheme"     : "dark" //default or dark
    },
    "opts": {
        "template": "./node_modules/ink-docstrap/template",
        "encoding": "utf8",
        "destination": "./out/",
        "recurse": false,
        "lenient": true
    }
}

